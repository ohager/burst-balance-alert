{
  "env": {
    "API_USER": "@burst-balance-alert-user",
    "API_PASS": "@burst-balance-alert-pass",
    "BURST_PEER": "https://wallet.burst-alliance.org:8125"
  },
  "routes": [
    { "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Access-Control-Allow-Headers": "*"
      }
    },
    { "src": "/.*", "status": 404}
  ]
}
