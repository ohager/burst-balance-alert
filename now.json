{
  "functions": {
    "api/status.ts": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/redirect.ts": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/hashicon.ts": {
      "memory": 128,
      "maxDuration": 20
    },
    "api/check-balance.ts": {
      "memory": 128,
      "maxDuration": 60
    }
  },
  "env": {
    "API_USER": "@burst-balance-alert-user",
    "API_PASS": "@burst-balance-alert-pass",
    "AWS_ID": "@aws-key-id",
    "AWS_SECRET": "@aws-secret-key",
    "DISCORD_WEBHOOK_API": "https://discordapp.com/api/webhooks",
    "BURST_PEER": "https://wallet.burst-alliance.org:8125",
    "BURST_EXPLORER": "https://explorer.burstcoin.network"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/.*",
      "status": 404
    }
  ]
}
